{
    "name": "grantholle/pear-services-yadis",
    "description": "A psr-4 compliant port of pear/services_yadis, an implementation of the Yadis Specification 1.0 protocol",
    "homepage": "https://github.com/grantholle/pear-services-yadis",
    "support": {
        "issues": "https://github.com/grantholle/pear-services-yadis/issues",
        "source": "https://github.com/grantholle/pear-services-yadis"
    },
    "license": "BSD-2-Clause",
    "type": "library",
    "authors": [
        {
            "email": "shupp@php.net",
            "name": "Bill Shupp",
            "role": "Lead"
        },
        {
            "name": "Grant Holle",
            "homepage": "https://grantholle.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Pear\\Services\\Yadis\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    },
    "require": {
        "grantholle/pear-http-request2": "^3.0",
        "grantholle/pear-net-url2": "^3.0",
        "php": "^7.2|^8.0",
        "ext-simplexml": "*",
        "ext-dom": "*"
    }
}
